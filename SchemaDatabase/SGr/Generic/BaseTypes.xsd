  <!--  generic Attribute Definitions  -->
  <complexType name="GenericAttributes">
    <annotation>
      <documentation>these attributes are defined for the generic interface for the application
        programmer. An attribute can be aligned to either a device, a functional Profile or a
        datapoint </documentation>
    </annotation>
    <all>
      <element name="maxVal" type="decimal" minOccurs="0">
        <annotation>
          <documentation> upper range limit. Unit:inherited</documentation>
        </annotation>
      </element>
      <element name="minVal" type="decimal" minOccurs="0">
        <annotation>
          <documentation> lower range limit. Unit:inherited</documentation>
        </annotation>
      </element>
      <element name="specialQualityRequirement" type="string" minOccurs="0">
        <annotation>
          <documentation>indicates quality requirements fullfilled like formal certifications</documentation>
        </annotation>
      </element>
      <element name="precisionPercent" type="sgr:PrecisionPercent" minOccurs="0">
        <annotation>
          <documentation> the precisionPercent of a measurement, calculation result or result of a
            controls process </documentation>
        </annotation>
      </element>
      <element name="stabilityFallback" type="sgr:StabilityFallback" minOccurs="0">
        <annotation>
          <documentation> A consumer or a generating system receives the permit for a load change
            for a certain period of time. This time is always set to 0 each time a confirmation
            message is received (HeartBeat). </documentation>
        </annotation>
      </element>
      <element name="smoothTransition" type="sgr:SmoothTransition" minOccurs="0">
        <annotation>
          <documentation> The time behavior of a transition from a power adjustment (positive as
            well as negative) can be determined by several time values, so that this starts with a
            random time delay, changes via a ramp and an expiry time with return to the initial
            value. </documentation>
        </annotation>
      </element>
      <element name="maxLatencyTimeMs" type="unsignedLong" minOccurs="0">
        <annotation>
          <documentation> Maximum time in milliseconds from capturing of measured value until ready
            at the external interface (i.e. analog-digital conversion time) </documentation>
        </annotation>
      </element>
      <element name="measuredValueType" type="sgr:MeasuredValueType" minOccurs="0">
        <annotation>
          <documentation> MeasValueType: type of measurement. Possbile values are "value", "min",
            max", "average", "stdDev" </documentation>
        </annotation>
      </element>
      <element name="measuredValueSource" type="sgr:MeasuredValueSource" minOccurs="0">
        <annotation>
          <documentation> Value source kind related to SGr level 6 applications. Potential values
            are measuredValue, calculatedValue, empiricalValue </documentation>
        </annotation>
      </element>
      <element name="sampleRateHz" type="unsignedLong" minOccurs="0">
        <annotation>
          <documentation> SampleRate in Hertz</documentation>
        </annotation>
      </element>
      <!-- TODO: describe generic attributes -->
      <element name="curtailment" type="double" minOccurs="0" />
      <element name="minLoad" type="double" minOccurs="0" />
      <element name="maxLockTimeMinutes" type="float" minOccurs="0" />
      <element name="minRunTimeMinutes" type="float" minOccurs="0" />
      <element name="valueByTimeTableMinutes" type="float" minOccurs="0" />
      <element name="flexAssistance" type="sgr:FlexAssistance" minOccurs="0">
        <annotation>
          <documentation> Systems with more than One communicator need a definition of the priority
            of the commands / demands for a flexibility requirement. This element defines the kind
            of a such a command (servicable for net (DSO), energy or system (TNO)) and its priority
            (SHALL / SHOULD / MAY) </documentation>
        </annotation>
      </element>
      <element name="unitConversionMultiplicator" type="float" minOccurs="0" />
    </all>
  </complexType>
  <complexType name="StabilityFallback">
    <annotation>
      <documentation> A consumer or a generating system receives the permit for a load change for a
        certain period of time. This time is always set to 0 each time a confirmation message is
        received (HeartBeat). </documentation>
    </annotation>
    <sequence>
      <element name="maxReceiveTime" type="float">
        <annotation>
          <documentation> Time in seconds. If the device does not recieve any communication within
            this specified time the device automatically initiates the fallback. 0 indicates no
            fallback will be performed. </documentation>
        </annotation>
      </element>
      <element name="initValue" type="decimal">
        <annotation>
          <documentation>Value of the reference variable at the start of the process cycle. Unit:
            inherited </documentation>
        </annotation>
      </element>
      <element name="fallbackValue" type="decimal">
        <annotation>
          <documentation>Value of the reference variable in the event of a communication failure .
            Unit:inherited </documentation>
        </annotation>
      </element>
    </sequence>
  </complexType>
  <complexType name="SmoothTransition">
    <annotation>
      <documentation> The time behavior of a transition from a power adjustment (positive as well as
        negative) can be determined by several time values, so that this starts with a random time
        delay, changes via a ramp and an expiry time with return to the initial value. </documentation>
    </annotation>
    <sequence>
      <element name="winTms" type="unsignedLong" minOccurs="0">
        <annotation>
          <documentation>indicates a time window in seconds in which the new operating mode is
            started randomly. The time window begins with the start command of the operating mode.
            The value 0 means immediate </documentation>
        </annotation>
      </element>
      <element name="rvrtTms" type="unsignedLong" minOccurs="0">
        <annotation>
          <documentation> determines how long the operating mode should be active in seconds. When
            the time has elapsed, the operating mode is automatically terminated. If rvrtTms = 0
            (standard value), the operating mode remains active until a new command is received. </documentation>
        </annotation>
      </element>
      <element name="rmpTms" type="unsignedLong" minOccurs="0">
        <annotation>
          <documentation>specifies how quickly the changes should be made in seconds. The
            corresponding value is gradually changed from the old to the new value in the specified
            time. </documentation>
        </annotation>
      </element>
    </sequence>
  </complexType>
  <complexType name="FlexAssistance">
    <sequence>
      <element name="assists" type="sgr:SGCPServiceType" />
      <element name="obligedTo" type="sgr:ObligLVlType" />
    </sequence>
  </complexType>


